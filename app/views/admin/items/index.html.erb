<html>
<h1>商品一覧</h1>

<p><%= link_to "新規登録", new_admin_item_path %></p>

<table>  
  <tr>
    <th>商品ID</th>
    <th>商品名</th>
    <th>税抜価格</th>
  </tr>  

<% @items.each do |item| %>
<tr>
  <td><%= item.id %></td>
  <td><%= link_to admin_item_path(item.id) do %>
  <%= item.name %>
  <% end %></td>
  <td><%= item.price %></td>
<% end %>
</tr>
</table>
</html>